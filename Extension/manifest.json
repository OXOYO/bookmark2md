{
  "manifest_version": 2,
  "name": "Bookmark2md",
  "version": "1.0.7",
  "default_locale": "zh_CN",
  "description": "__MSG_description__",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Bookmark2md",
    "default_popup": "html/options.html"
  },
  "author": "OXOYO",
  "permissions": [
    "bookmarks",
    "notifications",
    "tabs",
    "https://github.com/",
    "storage",
    "activeTab",
    "identity"
  ],
  "homepage_url": "https://github.com/OXOYO/bookmark2md",
  "content_scripts": [
    {
      "matches": ["https://github.com/robots.txt*"],
      "js": ["lib/oauth2/oauth2_inject.js"],
      "run_at": "document_start"
    }
  ],
  "options_page": "html/options.html",
  "options_ui": {
    "page": "html/options.html",
    "chrome_style": false
  },
  "default_locale": "zh_CN",
  "web_accessible_resources": [
    "lib/oauth2/oauth2.html"
  ]
}
